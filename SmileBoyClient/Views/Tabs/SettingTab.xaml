<Page x:Class="SmileBoyClient.Views.Tabs.SettingTab"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SmileBoyClient.Views.Tabs"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SettingTab"
      DataContext="{Binding SettingTab, Source={StaticResource ViewModelLocator}}">

    <Grid>
        <StackPanel Height="450" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Image Margin="0" Source="/Assets/Page.png" Width="400"></Image>
            <TextBlock Margin="0 0 0 0"
                   Text="CURRENTLY UNDER DEVELOPMENT"
                   FontSize="12"
                   FontFamily="{StaticResource MontserratBold}"
                   Foreground="#ca3e47"
                   HorizontalAlignment="Center"
                   VerticalAlignment="Center"/>
        </StackPanel>
    </Grid>
</Page>

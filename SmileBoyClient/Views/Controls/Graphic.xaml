<UserControl x:Class="SmileBoyClient.Views.Controls.Graphic"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SmileBoyClient.Views.Controls" xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf"
             mc:Ignorable="d" 
             d:DesignHeight="350" d:DesignWidth="700">
    <lvc:CartesianChart Height="228"  Series="{Binding Series}">
        <lvc:CartesianChart.SeriesColors>
            <lvc:ColorsCollection>
                <Color>#E85270</Color>
            </lvc:ColorsCollection>
        </lvc:CartesianChart.SeriesColors>
    </lvc:CartesianChart>
</UserControl>
